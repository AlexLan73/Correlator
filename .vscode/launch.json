{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Correlator (Release)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Correlator",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "CMAKE_BUILD_TYPE",
                    "value": "Release"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build (Release)",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false
            },
            "comment": "Release build: DEBUG_LOG and VERBOSE_LOG are disabled. Only INFO_LOG, ERROR_LOG, and std::cout from main.cpp are shown."
        },
        {
            "name": "Launch Correlator (Debug)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/Correlator",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build (Debug)",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Launch Correlator (RelWithDebInfo)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_reldeb/Correlator",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build (RelWithDebInfo)",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Attach to Process",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/build_debug/Correlator",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}